{
  "name": "resource-collection",
  "version": "2.2.2",
  "description": "",
  "main": "app.js",
  "private": true,
  "scripts": {
    "start": "node --require esbuild-register src/app.ts",
    "build": "node esbuild.mjs",
    "serve:start": "pm2 start mocksite/server.js --name rsrc_col",
    "serve:stop": "pm2 delete rsrc_col",
    "lint": "eslint .",
    "fix": "prettier . --write && eslint . --fix",
    "test": "vitest",
    "release": "standard-version",
    "preinstall": "typesync || :",
    "prepare": "husky install"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": "eslint src --fix",
    "src/**/*": "prettier src --write"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "puppeteer": "^21.2.1"
  },
  "devDependencies": {
    "@types/eslint": "^8.44.6",
    "@types/lint-staged": "^13.2.1",
    "@types/node": "^20.6.1",
    "@types/standard-version": "^7.1.1",
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest",
    "bun-types": "latest",
    "cz-emoji": "1.3.2-canary.2",
    "esbuild": "^0.19.4",
    "esbuild-register": "^3.5.0",
    "eslint": "^8.51.0",
    "husky": "^8.0.0",
    "lint-staged": "^15.0.2",
    "pm2": "^5.3.0",
    "prettier": "^3.0.3",
    "standard-version": "^9.5.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2",
    "typesync": "^0.11.1",
    "vite": "^4.4.11",
    "vitest": "^0.34.6"
  }
}
